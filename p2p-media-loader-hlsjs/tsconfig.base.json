{
  "compilerOptions": {
    "target": "es2017",
    "module": "NodeNext",
    "strict": true,
    "moduleResolution": "NodeNext",
    "declaration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
  },
}
